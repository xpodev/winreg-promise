{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["ESNext"],
    "module": "commonjs",
    "rootDir": "./src",
    "declaration": false,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": false
  }
}
